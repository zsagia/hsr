  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "build": "webpack",
    "build:aot": "ngc -p src/tsconfig.aot.json",
    "build:webpack-aot": "webpack --config webpack.aot.config.js ",
    "build:webpack-jit": "webpack --config webpack.jit.config.js ",
    "rollup": "rollup -c rollup-config.js",
    "deploy": "npm run buildProd && firebase deploy",
    "lint": "ng lint",
    "tslint": "tslint -t stylish -c tslint.json --project src/tsconfig.app.json --type-check",
    "test": "karma start ./karma.conf.js",
    "e2e": "protractor ./protractor.conf.js",
    "compodoc": "compodoc -p src/tsconfig.app.json -s",
    "postinstall": "typings install",
    "update-webdriver": "webdriver-manager update",
    "repl": "protractor --elementExplorer",
    "phantom": "phantomjs --webdriver=9515",
    "elementexplorer": "node node_modules/protractor/bin/elementexplorer.js http://localhost:4200",
    "start": "webpack-dev-server --port=4200",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
