{
  "name": "hsr",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port 4200 --live-reload",
    "build": "ng build",
    "buildProd": "ng build --target=production --environment=prod",
    "deploy": "npm run buildProd && firebase deploy",
    "lint": "ng lint",
    "tslint": "tslint -t stylish -c tslint.json --project src/tsconfig.app.json --type-check",
    "test": "ng test",
    "e2e": "ng e2e",
    "compodoc": "compodoc --tsconfig src/tsconfig.app.json --output docs --serve",
    "postinstall": "typings install",
    "update-webdriver": "webdriver-manager update",
    "repl": "protractor --elementExplorer",
    "phantom": "phantomjs --webdriver=9515",
    "elementexplorer": "node node_modules/protractor/bin/elementexplorer.js http://localhost:4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0-rc.5",
    "@angular/common": "^4.0.0-rc.5",
    "@angular/compiler": "^4.0.0-rc.5",
    "@angular/core": "^4.0.0-rc.5",
    "@angular/forms": "^4.0.0-rc.5",
    "@angular/http": "^4.0.0-rc.5",
    "@angular/router": "^4.0.0-rc.5",
    "@angular/platform-browser": "^4.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.5",
    "@angular/platform-server": "^4.0.0-rc.5",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "angularfire2": "^2.0.0-beta.8",
    "codemirror": "^5.20.2",
    "core-js": "^2.4.1",
    "dropzone": "^4.3.0",
    "firebase": "^3.7.3",
    "font-awesome": "^4.7.0",
    "froala-editor": "^2.5.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "ng2-slim-loading-bar": "^2.4.0",
    "rxjs": "^5.2.0",
    "tinymce": "^4.5.5",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.4",
    "@angular/compiler-cli": "^4.0.0-rc.5",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.45",
    "@types/jquery": "^2.0.41",
    "@types/node": "^7.0.8",
    "codelyzer": "^3.0.0-beta.4",
    "compodoc": "0.0.41",
    "firebase-tools": "^3.5.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.1",
    "protractor-html-screenshot-reporter": "0.0.21",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "typings": "^2.1.0"
  }
}
