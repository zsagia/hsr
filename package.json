{
  "name": "hsr",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port 4200 --live-reload",
    "build": "ng build",
    "build:prod": "ng build --prod --aot true",
    "deploy": "npm run build:prod && firebase deploy",
    "lint": "ng lint",
    "tslint": "tslint -t stylish -c tslint.json --project src/tsconfig.app.json --type-check",
    "test": "ng test",
    "e2e": "ng e2e",
    "compodoc": "compodoc --tsconfig src/tsconfig.app.json --output docs --serve",
    "postinstall": "typings install",
    "update-webdriver": "webdriver-manager update",
    "repl": "protractor --elementExplorer",
    "phantom": "phantomjs --webdriver=9515",
    "elementexplorer": "node node_modules/protractor/bin/elementexplorer.js http://localhost:4200",
    "libs": "npm install",
    "libs:renew": "rm -rf node_modules && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/platform-server": "^4.2.4",
    "@angular/router": "^4.2.4",
    "angular-linky": "^1.2.2",
    "angularfire2": "^4.0.0-rc.1",
    "core-js": "^2.4.1",
    "dropzone": "^5.1.1",
    "firebase": "^4.0.0",
    "font-awesome": "^4.7.0",
    "froala-editor": "^2.6.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "ng-gallery": "^0.7.1",
    "ng2-slim-loading-bar": "^4.0.0",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.47",
    "@types/jquery": "^2.0.45",
    "@types/node": "^7.0.29",
    "codelyzer": "^3.1.1",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.2",
    "protractor-html-screenshot-reporter": "0.0.21",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "typings": "^2.1.1"
  }
}
