{
  "name": "hsr",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port 4200 --live-reload",
    "build": "ng build",
    "build:prod": "ng build --target=production --environment=prod",
    "deploy": "npm run build:prod && firebase deploy",
    "lint": "ng lint",
    "tslint": "tslint -t stylish -c tslint.json --project src/tsconfig.app.json --type-check",
    "test": "ng test",
    "e2e": "ng e2e",
    "compodoc": "compodoc --tsconfig src/tsconfig.app.json --output docs --serve",
    "postinstall": "typings install",
    "update-webdriver": "webdriver-manager update",
    "repl": "protractor --elementExplorer",
    "phantom": "phantomjs --webdriver=9515",
    "elementexplorer": "node node_modules/protractor/bin/elementexplorer.js http://localhost:4200",
    "libs": "npm install",
    "libs:renew": "rm -rf node_modules && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/platform-server": "^4.1.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "angularfire2": "^2.0.0-beta.8",
    "codemirror": "^5.20.2",
    "core-js": "^2.4.1",
    "dropzone": "^4.3.0",
    "firebase": "^3.9.0",
    "font-awesome": "^4.7.0",
    "froala-editor": "^2.5.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "ng2-slim-loading-bar": "^4.0.0",
    "rxjs": "^5.3.0",
    "tinymce": "^4.5.7",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1",
    "@angular/compiler-cli": "^4.1.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.47",
    "@types/jquery": "^2.0.41",
    "@types/node": "^7.0.14",
    "codelyzer": "^3.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.8",
    "firebase-tools": "^3.6.1",
    "jasmine-core": "^2.6.1",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.1",
    "protractor-html-screenshot-reporter": "0.0.21",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "typings": "^2.1.1"
  }
}
